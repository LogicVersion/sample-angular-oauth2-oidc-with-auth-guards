    "version": "0.2.0",
    "configurations": [
      {
        "name": "Attach to Chrome",
        "port": 4200,
        "request": "attach",
        "type": "chrome",
        "url": "https://localhost:4200",
        "webRoot": "${workspaceFolder}"
      },
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "chrome", //pwa-chrome
      "timeout": 20000,
      "url": "https://localhost:4200",
      "webRoot": "${workspaceFolder}"
    },
      {
        "type": "browser-preview",
        "request": "launch",
        "name": "Browser Preview: Launch",
        "url": "https://localhost:4200"
      },

    ]
}
